<%= @ticker.name %>

<%= stuff = {} %>
<%= @ticker.prices.order(:created_at).each do |price_obj| %>
  <%= datet = price_obj.created_at %>
  <%= val = price_obj.value %>
  <%= stuff[datet] = val %>
<% end %>

<%= puts stuff %>
<%# line_chart Hash["test": 0.5, "test2": 0.75, "test3": 0.63] %>
<%= line_chart stuff %>
